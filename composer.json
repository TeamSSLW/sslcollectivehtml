{
  "name": "sslcollective/html",
  "description": "HTML and Form Builders for the Laravel Framework.",
  "license": "MIT",
  "version": "v1.0.3",
  "type": "library",
  "authors": [
    {
      "name": "Md Ismail Hossen",
      "email": "hossenismail29@gmail.com"
    },
    {
      "name": "Abdul Latif",
      "email": "abdul.latif@sslwireless.com"
    }
  ],
  "require": {
    "php": "^8.3",
    "illuminate/contracts": "^12.0",
    "illuminate/routing": "^12.0",
    "illuminate/session": "^12.0",
    "illuminate/support": "^12.0",
    "illuminate/view": "^12.0"
  },
  "require-dev": {
    "illuminate/database": "^12.0",
    "phpunit/phpunit": "^11.0",
    "orchestra/testbench": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "SSLCollective\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "SSLCollective\\HtmlServiceProvider"
      ],
      "aliases": {
        "Form": "SSLCollective\\FormFacade",
        "Html": "SSLCollective\\HtmlFacade"
      }
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
