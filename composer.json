{
  "name": "sslcollective/html",
  "description": "HTML and Form Builders for the Laravel Framework.",
  "license": "MIT",
  "version": "1.0.2",
  "type": "library",
  "authors": [
    {
      "name": "Md Ismail Hossen",
      "email": "hossenismail29@gmail.com"
    },
    {
      "name": "Abdul Latif",
      "email": "abdul.latif@sslwireless.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "illuminate/contracts": "^11.0",
    "illuminate/routing": "^11.0",
    "illuminate/session": "^11.0",
    "illuminate/support": "^11.0",
    "illuminate/view": "^11.0"
  },
  "require-dev": {
    "illuminate/database": "^11.0",
    "phpunit/phpunit": "^10.0",
    "orchestra/testbench": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "SSLCollective\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "SSLCollective\\HtmlServiceProvider"
      ],
      "aliases": {
        "Form": "SSLCollective\\FormFacade",
        "Html": "SSLCollective\\HtmlFacade"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
