{
    "name": "sslcollective/html",
    "description": "HTML and Form Builders for the Laravel Framework.",
    "license": "MIT",
    "authors": [
        {
            "name": "Md Ismail Hossen",
            "email": "hossenismail29@gmail.com"
        },
        {
            "name": "Abdul Latif",
            "email": "abdul.latif@sslwireless.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "illuminate/routing": "v11.4.0.*",
        "illuminate/session": "v11.4.0.*",
        "illuminate/support": "v11.4.0.*",
        "illuminate/view": "v11.4.0.*"
    },
    "require-dev": {
        "illuminate/database": "v11.4.0.*"
    },
    "autoload": {
        "psr-4": {
            "SSLCollective\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "SSLCollective\\HtmlServiceProvider"
            ],
            "aliases": {
                "Form": "SSLCollective\\FormFacade",
                "Html": "SSLCollective\\HtmlFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
